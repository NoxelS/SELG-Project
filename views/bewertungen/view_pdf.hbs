    <style>
        html,
        body {
            width: 0px;
            height: 0px;
            margin: 0px;
            padding: 0px;
            color: black;
            font-family: arial;

        }

        .main {
            width: 640px;
            padding: 0px;
            margin-left: 1cm;
        }

        .rating-text {
            color: #666666;
            font-size: medium;
        }

        .txt-center {
            text-align: center;
        }

        .hide {
            display: none;
        }

        .clear {
            float: none;
            clear: both;
            width: 0px;
            height: 0px;
        }

        .rating {
            width: 90px;
            unicode-bidi: bidi-override;
            direction: rtl;
            position: relative;
        }

        .rating>label {

            display: inline;
            padding: 0;
            margin: 0;
            position: relative;
            width: 1.1em;
            cursor: pointer;
            color: #000;
        }

        .rating>label:hover,
        .rating>label:hover~label,
        .rating>input.radio-btn:checked~label {
            color: transparent;
        }

        .rating>label:hover:before,
        .rating>label:hover~label:before,
        .rating>input.radio-btn:checked~label:before,
        .rating>input.radio-btn:checked~label:before {
            content: "\2605";
            position: absolute;
            left: 0;
            color: #FFD700;
        }

        .rating-fix {
            display: inline;
            padding: 0px;
            margin: 0px;
            position: relative;
            width: 1.1em;
            color: #000;
        }

        .filled {
            font-size: medium;
            content: "\2605";
            color: #FFD700;
        }

        .img-header {
            margin: 0px;
            padding: 0px;
            height: 80px;
            width: 90px;
            position: fixed;
            top: 0px;
            right: 10px;
        }

        .sozialverhalten-tabelle {
            border: 0.1em solid black;
        }

        table {
            width: 100%;
            border-collapse: collapse;
        }

        .table-title {
            text-align: left;
            font-size: 18px;
            padding: 28px 28px 28px 28px;
            width: 300px;
        }

        td,
        tr,
        th {
            border: 0.1em solid black;
            padding: 2px 2px 2px 2px;
        }

        .star {
            vertical-align: bottom;
            width: 30px;
            height: 74px;
        }

        .no-border {
            border: 0px;
        }

        .zahl {
            padding: 0px 10px 0px 10px
        }

        .kreuz{
            text-align: center;
        }
    </style>
</head>

